   <define xmlns="http://relaxng.org/ns/structure/1.0" name="ref">
      <element name="ref">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="ref.content"/>
         <ref name="ref.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="ref.content">
      <rng:text xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="ref.localattributes">
      <ref name="IDLangTgtDtyp.attributes"/>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="ref_crossReference">
         <sch:rule context="ref[@type='crossReference']">
            <sch:assert test="parent::termEntry or parent::tig"> A cross-reference can occur only at
               the concept (termEntry) level or the term (tig) level.</sch:assert>
         </sch:rule>
      </sch:pattern>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <rng:choice>
            <rng:value>crossReference</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (crossReference) A pointer to another related location, such as another entry or
               another term, within the same TBX-Basic document instance. The ref element with a
               type attribute value of cross-reference shall also have a target attribute to point
               to the identifier of the entry or term.</a:documentation>
         </rng:choice>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="term">
      <element name="term">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="term.content"/>
         <ref name="term.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="term.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="entity.basicText"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="term.localattributes">
      <ref name="id.attributes"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termEntry">
      <element name="termEntry">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="termEntry.content"/>
         <ref name="termEntry.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termEntry.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="entity.auxInfo"/>
      <rng:oneOrMore xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:ref name="langSet"/>
      </rng:oneOrMore>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termEntry.localattributes">
      <ref name="id.attributes"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termNote">
      <element name="termNote">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="termNote.content"/>
         <ref name="termNote.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termNote.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="entity.noteText"/>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_administrativeStatus">
         <sch:rule context="termNote[@type='administrativeStatus']">
            <sch:assert
               test=".=         'preferredTerm-admn-sts' or .='admittedTerm-admn-sts' 
            or .='deprecatedTerm-admn-sts' or .='supersededTerm-admn-sts'"
               >The values of administrativeStatus must be one of: preferredTerm-admn-sts, admittedTerm-admn-sts,
                deprecatedTerm-admn-sts, or supersededTerm-admn-sts. For TBX-Basic software applications, the display values can be simplified to:
               preferred, admitted, notRecommended and obsolete.
            </sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_geographicalUsage">
         <sch:rule context="termNote[@type='geographicalUsage']">
            <sch:assert test=".=not(*)">The geographical usage must be expressed in plainText.
            </sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_grammaticalGender">
         <sch:rule context="termNote[@type='grammaticalGender']">
            <sch:assert test="(.='masculine' or .='feminine' or .='neuter' or .='other')"> The
               gender must be masculine, feminine, neuter, or other. Gender should be specified at
               the term level (tig).</sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_partOfSpeech">
         <sch:rule context="termNote[@type='partOfSpeech']">
            <sch:assert
               test=". = 'noun' or .='verb' or .='adjective' or .='adverb' or .='properNoun' or .='other'"
               >The part of speech must be either noun, verb, adjective, adverb, properNoun, or
               other. </sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_termLocation">
         <sch:rule context="termNote[@type='termLocation']">
            <sch:assert test=".=not(*)">The termLocation must be a plainText value. It can be
               specified only at the term (tig) level.</sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="termNote_termType">
         <sch:rule context="termNote[@type='termType']">
            <sch:assert
               test=". = 'abbreviation' or .='acronym' or .='fullForm' or .='variant' 
            or .='shortForm' or .='phrase'"
               >The type of term can only be one of the following values: abbreviation, acronym,
               fullForm, variant, shortForm, phrase.</sch:assert>
         </sch:rule>
      </sch:pattern>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="termNote.localattributes">
      <ref name="IDLangTgtDtyp.attributes"/>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates
            the data-category type of the termNote, such as partOfSpeech or termType.</a:documentation>
         <rng:choice>
            <rng:value>administrativeStatus</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (Administrative status) The status of a term with respect to usage. The element
               termNote when its type attribute has the value administrativeStatus must contain one
               of the following values: * preferred * admitted * notRecommended * obsolete. </a:documentation>
            <rng:value>geographicalUsage</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (geographicalUsage) A code identifying any specific geographical area where the term
               is used, for instance, to differentiate between UK English and US English. Preferred
               values are language codes combined with geographical codes, from IETF RFC 4646 or its
               successor, as identified in IETF BCP 47. For example: en-US for American English. The
               element content shall be expressed in plainText. </a:documentation>
            <rng:value>grammaticalGender</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (Grammatical gender) The gender of the word. The value of the termNote element when
               it has a type attribute value of grammaticalGender must be one of: masculine,
               feminine, neuter, or other.</a:documentation>
            <rng:value>partOfSpeech</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Part of
               speech) A category assigned to a word based on its grammatical and semantic
               properties. The element content shall be one of: noun, verb, adjective, adverb,
               properNoun, other. Only one value shall be permitted for each term in a given entry.</a:documentation>
            <rng:value>termLocation</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (termLocation) A location in a document, computer file, or other information medium,
               where the term frequently occurs, such as a user interface object (in software), a
               packaging element, a component in an industrial process, and so forth. The element
               content shall be expressed in plainText, and preferably be restricted to a set of
               picklist values. See the TBX-Basic specification document for a list of suggested
               values for software localization environments.</a:documentation>
            <rng:value>termType</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (termType) A categorization of a term with respect to formal representations of terms
               or where or how they are used, for example, acronym. For TBX-Basic, the content of
               the termNote element when it has a type attribute value of termType shall be one of
               the following: * abbreviation * acronym * fullForm * shortForm * variant * phrase.
               Refer to the TBX-Basic specification for definitions of the above term
            types.</a:documentation>
         </rng:choice>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="tig">
      <element name="tig">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="tig.content"/>
         <ref name="tig.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="tig.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="term"/>
      <rng:zeroOrMore xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:ref name="termNote"/>
      </rng:zeroOrMore>
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="entity.auxInfo"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="tig.localattributes">
      <ref name="id.attributes"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transac">
      <element name="transac">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="transac.content"/>
         <ref name="transac.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transac.content">
      <rng:text xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0"/>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="transac_transactionType">
         <sch:rule context="transac[@type='transactionType']">
            <sch:assert test=". =        'origination' or .='modification' ">The content of this
               element indicates the type of transaction and for TBX-Basic must be one of the
               following values: origination, modification. </sch:assert>
         </sch:rule>
      </sch:pattern>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transac.localattributes">
      <ref name="IDLangTgtDtyp.attributes"/>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
            attribute value transactionType ensures that the content of the transac element will be
            a transaction type.</a:documentation>
         <rng:choice>
            <rng:value>transactionType</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (transactionType) A record that indicates the stage of the entry within the overall
               process of creation, approval, and use of a terminology entry.</a:documentation>
         </rng:choice>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacGrp">
      <element name="transacGrp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="transacGrp.content"/>
         <ref name="transacGrp.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacGrp.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="transac"/>
      <rng:zeroOrMore xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:choice>
            <rng:ref name="transacNote"/>
            <rng:ref name="date"/>
         </rng:choice>
      </rng:zeroOrMore>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacGrp.localattributes">
      <ref name="id.attributes"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacNote">
      <element name="transacNote">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="transacNote.content"/>
         <ref name="transacNote.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacNote.content">
      <rng:text xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="transacNote.localattributes">
      <ref name="IDLangTgtDtyp.attributes"/>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <rng:choice>
            <rng:value>responsibility</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (responsibility) The name of a person or entity associated with a terminology
               management transaction. The transacNote element with a type attribute value of
               responsibility can also have a target attribute to point to the identifier of a
               bibliographic entry in a refObject that contains information about the person or
               entity.</a:documentation>
         </rng:choice>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="xref">
      <element name="xref">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="xref.content"/>
         <ref name="xref.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="xref.content">
      <rng:text xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0"/>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="xref_xGraphic">
         <sch:rule context="xref[@type='xGraphic']">
            <sch:assert test="parent::termEntry"> Graphics must be inserted at the termEntry
            level.</sch:assert>
         </sch:rule>
      </sch:pattern>
      <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0"
         name="xref_externalCrossReference">
         <sch:rule context="xref[@type='externalCrossReference']">
            <sch:assert test="parent::termEntry or parent::tig">External cross references can occur
               at the concept (termEntry) or term (tig) level.</sch:assert>
         </sch:rule>
      </sch:pattern>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="xref.localattributes">
      <ref name="id.attributes"/>
      <rng:attribute name="target">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The URI of
            the external cross reference or graphic file.</a:documentation>
         <text/>
      </rng:attribute>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <rng:choice>
            <rng:value>externalCrossReference</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (externalCrossReference) A term or other terminological resource that is external to
               the terminological database or TBX document instance. In TBX-Basic it can be a
               pointer to external reference information or explanatory text about the term or
               concept, such as a Wikipedia article.</a:documentation>
            <rng:value>xGraphic</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
               (xGraphic) External graphic file.</a:documentation>
         </rng:choice>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martif">
      <element name="martif">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="martif.content"/>
         <ref name="martif.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martif.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="martifHeader"/>
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="text"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martif.localattributes">
      <rng:attribute name="xml:lang">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates
            the default language of the entire martif element content. The allowed values of the
            xml:lang attribute are found in IETF RFC 4646 or its successor, as indicated in IETF BCP
            47. This attribute is required for the martif element.</a:documentation>
         <text/>
      </rng:attribute>
      <rng:attribute name="type">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates
            the type of martif document. For TBX-Basic compliant files, use the value "TBX-Basic".</a:documentation>
         <text/>
      </rng:attribute>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martifHeader">
      <element name="martifHeader">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="martifHeader.content"/>
         <ref name="martifHeader.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martifHeader.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="fileDesc"/>
      <rng:optional xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:ref name="encodingDesc"/>
      </rng:optional>
      <rng:optional xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:ref name="revisionDesc"/>
      </rng:optional>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="martifHeader.localattributes">
      <ref name="id.attributes"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="p">
      <element name="p">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="p.content"/>
         <ref name="p.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="p.content">
      <rng:ref xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://www.ascc.net/xml/schematron"
         xmlns:tei="http://www.tei-c.org/ns/1.0" name="entity.noteText"/>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="p.localattributes">
      <ref name="idlang.attributes"/>
      <rng:optional>
         <rng:attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <rng:value>XCSURI</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The URI
               of the XCS file that constrains the content of the TBX document instance. You can
               point to a local copy of the XCS file, or you can point to the copy on the LISA Web
               site (http://www.lisa.org/fileadmin/standards/tbx_basic/TBXBasicXCSV02.XCS). Note: if
               you are using the Relax NG schema for validation, the XCS file is not used. However,
               to comply with the TBX standard (ISO 30042), a TBX-Basic document instance must have
               an XCS file for reference purposes.</a:documentation>
         </rng:attribute>
      </rng:optional>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="fileDesc">
      <element name="fileDesc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="fileDesc.content"/>
         <ref name="fileDesc.localattributes"/>
      </element>
   </define>
   <define xmlns="http://relaxng.org/ns/structure/1.0" name="fileDesc.content">
      <rng:optional xmlns="http://www.tei-c.org/ns/1.0"
         xmlns:sch="http://www.ascc.net/xml/schematron" xmlns:tei="http://www.tei-c.org/ns/1.0">
         <rng:ref name="titleStmt"/>
      </rng:optional>
      <rng:optional xmlns="http://www.tei-c.org/ns/1.0"
